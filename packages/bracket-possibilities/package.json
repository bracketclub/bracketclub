{
  "name": "bracket-possibilities",
  "description": "Get the best possible outcomes for a bracket based on a partially complete master bracket.",
  "version": "1.2.1",
  "author": "Luke Karrys <luke@lukekarrys.com>",
  "bugs": {
    "url": "https://github.com/tweetyourbracket/bracket-possibilities/issues"
  },
  "dependencies": {
    "bracket-data": "^4.4.0",
    "bracket-scorer": "^3.4.1",
    "bracket-updater": "^3.4.1",
    "bracket-validator": "^3.1.0",
    "lodash": "^4.17.2"
  },
  "devDependencies": {
    "@tap-format/spec": "^0.2.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-tape-runner": "^2.0.1",
    "git-validate": "^2.2.2",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  },
  "files": [
    "lib/*.js"
  ],
  "homepage": "https://github.com/tweetyourbracket/bracket-possibilities#readme",
  "keywords": [
    "bracket",
    "ncaa",
    "tweetyourbracket"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tweetyourbracket/bracket-possibilities.git"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "lint": "standard",
    "prepublish": "npm run compile",
    "test": "babel-tape-runner test/*.js | tap-format-spec",
    "validate": "npm ls"
  },
  "standard": {
    "ignore": [
      "lib/*"
    ]
  }
}

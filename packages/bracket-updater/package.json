  },
  "bugs": {
    "url": "http://github.com/tweetyourbracket/bracket-updater/issues"
  },
  "dependencies": {
    "bracket-data": "~2.0.3",
    "bracket-validator": "~2.0.1",
    "lodash-node": "~2.4.1"
  },
  "devDependencies": {
    "bracket-generator": "~1.0.0",
    "mocha": "~1.17.1",
    "precommit-hook": "~0.3.10"
  },
  "homepage": "http://github.com/tweetyourbracket/bracket-updater",
  "keywords": [
    "ncaa",
